<?xml version="1.0"?>
<openerp>
    <data>
        
        <record id="view_product_form_cbk_analytic_accounting" model="ir.ui.view">
            <field name="name">product.form.cbk.analytic.accounting</field>
            <field name="model">product.product</field>
            <field name="inherit_id" ref="product.product_normal_form_view"/>
            <field name="priority" eval="18"/>
            <field name="arch" type="xml">
                <xpath expr="/form/sheet/notebook" position="inside">
                    <page string="Analytical Accounting Manager">
                        <group>                          
                            <field name="packaging_unit_cost"/>
                            <field name="profit"/>
                        </group>
                    </page>
                </xpath>              
            </field>
        </record>   
        
    </data>
</openerp>
